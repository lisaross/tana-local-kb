{
  "name": "tana-local-kb",
  "version": "0.1.0",
  "type": "module",
  "description": "Transform your Tana database into a locally-hosted, AI-queryable knowledge system",
  "author": "Lisa Ross <hello@lisaross.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lisaross/tana-local-kb.git"
  },
  "scripts": {
    "dev": "bunx vite",
    "dev:all": "bunx concurrently \"bun run server\" \"bun run dev\" \"bun run chroma\"",
    "server": "bun run server/src/index.ts",
    "build": "bunx vite build",
    "preview": "bunx vite preview",
    "start": "bun run server/src/index.ts",
    "import": "bun run scripts/import.ts",
    "embed": "bun run scripts/embed.ts",
    "migrate": "bun run scripts/migrate.ts",
    "chroma": "uv run python scripts/chroma_service.py",
    "test": "bun test",
    "test:e2e": "bunx playwright test",
    "test:perf": "bun test tests/performance/",
    "type-check": "bunx tsc --noEmit",
    "lint": "bunx eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "bunx eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "bunx prettier --write .",
    "db:inspect": "bun run scripts/db-inspect.ts",
    "db:query": "bun run scripts/db-query.ts",
    "chroma:inspect": "bun run scripts/chroma-inspect.ts",
    "chroma:query": "bun run scripts/chroma-query.ts",
    "profile": "bun run scripts/profile.ts",
    "monitor": "bun run scripts/monitor.ts",
    "validate-json": "bun run scripts/validate-json.ts",
    "test-parser": "bun run scripts/test-parser.ts"
  },
  "dependencies": {
    "@hono/trpc-server": "^0.3.2",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-router": "^1.58.3",
    "@tanstack/react-table": "^8.11.2",
    "@trpc/client": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "hono": "^3.11.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4",
    "@radix-ui/react-command": "^1.0.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "lucide-react": "^0.294.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "stream-json": "^1.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "playwright": "^1.40.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "@types/stream-json": "^1.7.7"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "keywords": [
    "tana",
    "knowledge-base",
    "ai",
    "semantic-search",
    "local",
    "ollama",
    "chromadb",
    "bun",
    "typescript"
  ]
}